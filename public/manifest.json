{
  "name": "Pushpin Clipper",
  "key": "",
  "description": "Send the contents of the active tab to Pushpin.",
  "version": "0.2",
  "icons": { "128": "icon-128.png" },
  "permissions": ["background", "nativeMessaging", "activeTab", "contextMenus", "scripting"],
  "host_permissions": [
    "*://*/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Push to Pushpin"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "windows": "Ctrl+Shift+A",
        "mac": "Command+Shift+A",
        "chromeos": "Ctrl+Shift+A",
        "linux": "Ctrl+Shift+A"
      }
    }
  },
  "manifest_version": 3
}
